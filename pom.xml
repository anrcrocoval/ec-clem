<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>fr.univ-nantes</groupId>
    <artifactId>ec-clem</artifactId>
    <version>1.0-SNAPSHOT</version>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>

    <dependencies>
        <dependency>
            <groupId>fr.univ-nantes</groupId>
            <artifactId>icy</artifactId>
            <version>0.0.0</version>
        </dependency>
        <dependency>
            <groupId>fr.univ-nantes</groupId>
            <artifactId>ezplug</artifactId>
            <version>0.0.0</version>
        </dependency>
        <dependency>
            <groupId>fr.univ-nantes</groupId>
            <artifactId>colorbar</artifactId>
            <version>0.0.0</version>
        </dependency>
        <dependency>
            <groupId>fr.univ-nantes</groupId>
            <artifactId>blocks</artifactId>
            <version>0.0.0</version>
        </dependency>
        <dependency>
            <groupId>fr.univ-nantes</groupId>
            <artifactId>tvdenoising</artifactId>
            <version>0.0.0</version>
        </dependency>
        <dependency>
            <groupId>fr.univ-nantes</groupId>
            <artifactId>filtertoolbox</artifactId>
            <version>0.0.0</version>
        </dependency>
        <dependency>
            <groupId>fr.univ-nantes</groupId>
            <artifactId>bioformats</artifactId>
            <version>0.0.0</version>
        </dependency>
        <dependency>
            <groupId>fr.univ-nantes</groupId>
            <artifactId>jogl</artifactId>
            <version>0.0.0</version>
        </dependency>
        <dependency>
            <groupId>fr.univ-nantes</groupId>
            <artifactId>jai</artifactId>
            <version>0.0.0</version>
        </dependency>
        <dependency>
            <groupId>fr.univ-nantes</groupId>
            <artifactId>vtk</artifactId>
            <version>0.0.0</version>
        </dependency>

        <dependency>
            <groupId>gov.nist.math</groupId>
            <artifactId>jama</artifactId>
            <version>1.0.3</version>
        </dependency>
        <dependency>
            <groupId>org.jfree</groupId>
            <artifactId>jfreechart</artifactId>
            <version>1.0.19</version>
        </dependency>
        <dependency>
            <groupId>net.imagej</groupId>
            <artifactId>ij</artifactId>
            <version>1.52n</version>
        </dependency>
        <dependency>
            <groupId>commons-io</groupId>
            <artifactId>commons-io</artifactId>
            <version>2.6</version>
        </dependency>
        <dependency>
            <groupId>com.google.guava</groupId>
            <artifactId>guava</artifactId>
            <version>27.1-jre</version>
        </dependency>
        <dependency>
            <groupId>org.openmicroscopy</groupId>
            <artifactId>ome-xml</artifactId>
            <version>6.0.0</version>
        </dependency>
        <dependency>
            <groupId>org.openmicroscopy</groupId>
            <artifactId>ome-common</artifactId>
            <version>6.0.0</version>
        </dependency>
        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-math3</artifactId>
            <version>3.6.1</version>
        </dependency>
        <dependency>
            <groupId>org.junit.jupiter</groupId>
            <artifactId>junit-jupiter-engine</artifactId>
            <version>5.3.1</version>
            <scope>test</scope>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.8.0</version>
                <configuration>
                    <source>8</source>
                    <target>8</target>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-install-plugin</artifactId>
                <version>3.0.0-M1</version>
                <executions>
                    <execution>
                        <id>install-icy</id>
                        <phase>clean</phase>
                        <configuration>
                            <groupId>fr.univ-nantes</groupId>
                            <artifactId>icy</artifactId>
                            <version>0.0.0</version>
                            <file>${project.basedir}/libs/icy/icy.jar</file>
                            <packaging>jar</packaging>
                            <generatePom>true</generatePom>
                        </configuration>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>install-ezplug</id>
                        <phase>clean</phase>
                        <configuration>
                            <groupId>fr.univ-nantes</groupId>
                            <artifactId>ezplug</artifactId>
                            <version>0.0.0</version>
                            <file>${project.basedir}/libs/icy/plugins/adufour/ezplug/EzPlug.jar</file>
                            <packaging>jar</packaging>
                            <generatePom>true</generatePom>
                        </configuration>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>install-vtk</id>
                        <phase>clean</phase>
                        <configuration>
                            <groupId>fr.univ-nantes</groupId>
                            <artifactId>vtk</artifactId>
                            <version>0.0.0</version>
                            <file>${project.basedir}/libs/icy/lib/vtk.jar</file>
                            <packaging>jar</packaging>
                            <generatePom>true</generatePom>
                        </configuration>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>install-colorbar</id>
                        <phase>clean</phase>
                        <configuration>
                            <groupId>fr.univ-nantes</groupId>
                            <artifactId>colorbar</artifactId>
                            <version>0.0.0</version>
                            <file>${project.basedir}/libs/icy/plugins/stef/tools/overlay/ColorBar.jar</file>
                            <packaging>jar</packaging>
                            <generatePom>true</generatePom>
                        </configuration>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>install-blocks</id>
                        <phase>clean</phase>
                        <configuration>
                            <groupId>fr.univ-nantes</groupId>
                            <artifactId>blocks</artifactId>
                            <version>0.0.0</version>
                            <file>${project.basedir}/libs/icy/plugins/adufour/blocks/Blocks.jar</file>
                            <packaging>jar</packaging>
                            <generatePom>true</generatePom>
                        </configuration>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>install-tvdenoising</id>
                        <phase>clean</phase>
                        <configuration>
                            <groupId>fr.univ-nantes</groupId>
                            <artifactId>tvdenoising</artifactId>
                            <version>0.0.0</version>
                            <file>${project.basedir}/libs/icy/plugins/nchenouard/tvdenoising/TVDenoising.jar</file>
                            <packaging>jar</packaging>
                            <generatePom>true</generatePom>
                        </configuration>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>install-filtertoolbox</id>
                        <phase>clean</phase>
                        <configuration>
                            <groupId>fr.univ-nantes</groupId>
                            <artifactId>filtertoolbox</artifactId>
                            <version>0.0.0</version>
                            <file>${project.basedir}/libs/icy/plugins/adufour/filtering/FilterToolbox.jar</file>
                            <packaging>jar</packaging>
                            <generatePom>true</generatePom>
                        </configuration>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>install-bioformats</id>
                        <phase>clean</phase>
                        <configuration>
                            <groupId>fr.univ-nantes</groupId>
                            <artifactId>bioformats</artifactId>
                            <version>0.0.0</version>
                            <file>${project.basedir}/libs/icy/lib/bioformats.jar</file>
                            <packaging>jar</packaging>
                            <generatePom>true</generatePom>
                        </configuration>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>install-jogl</id>
                        <phase>clean</phase>
                        <configuration>
                            <groupId>fr.univ-nantes</groupId>
                            <artifactId>jogl</artifactId>
                            <version>0.0.0</version>
                            <file>${project.basedir}/libs/icy/lib/jogl-all.jar</file>
                            <packaging>jar</packaging>
                            <generatePom>true</generatePom>
                        </configuration>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>install-jai</id>
                        <phase>clean</phase>
                        <configuration>
                            <groupId>fr.univ-nantes</groupId>
                            <artifactId>jai</artifactId>
                            <version>0.0.0</version>
                            <file>${project.basedir}/libs/icy/lib/jai_core.jar</file>
                            <packaging>jar</packaging>
                            <generatePom>true</generatePom>
                        </configuration>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>3.0.0-M3</version>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-shade-plugin</artifactId>
                <version>3.2.1</version>
                <executions>
                    <execution>
                        <phase>package</phase>
                        <goals>
                            <goal>shade</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>
</project>